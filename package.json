		"@types/elementtree": "^0.1.0",
		"aws-sdk": "^2.1040.0",
		"cross-fetch": "^3.1.5",
		"elementtree": "^0.1.7",
		"immutable": "3.8.2",
		"jszip": "^3.7.1",
		"lodash-es": "4.17.15",
		"mysql": "^2.17.1",
		"node-fetch": "^2.6.7",
		"sax": "1.2.4",
		"serverless-mysql": "^1.5.1",
		"tslib": "^1.9.0",
		"uuid": "^3.4.0"
	},
	"devDependencies": {
		"@types/aws-lambda": "8.10.31",
		"@types/chai": "4.2.1",
		"@types/jest": "24.0.18",
		"@types/jszip": "^3.1.7",
		"@types/mysql": "^2.15.7",
		"@types/node": "12.7.4",
		"@types/node-fetch": "^2.5.2",
		"@types/uuid": "^3.4.7",
		"@types/xmldoc": "^1.1.4",
		"@typescript-eslint/eslint-plugin": "^2.1.0",
		"@typescript-eslint/parser": "^2.1.0",
		"eslint": "^6.6.0",
		"eslint-config-prettier": "^6.5.0",
		"eslint-loader": "^3.0.2",
		"eslint-plugin-prettier": "^3.1.1",
		"jest": "^24.9.0",
		"prettier": "^1.19.1",
